{
	"name": "CopyMetadata",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": true,
					"secureInput": true
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"modifiedDatetimeEnd": {
								"value": "@utcnow()",
								"type": "Expression"
							},
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['extraction_date']"
								},
								"sink": {
									"name": "MD_INSERT_TIME",
									"type": "DateTimeOffset"
								}
							},
							{
								"source": {
									"path": "$['case_metadata_event_id']"
								},
								"sink": {
									"name": "CE_ID",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['ce_case_data_id']"
								},
								"sink": {
									"name": "CE_CASE_DATA_ID",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['ce_created_date']"
								},
								"sink": {
									"name": "CE_CREATED_DATE",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['ce_case_type_id']"
								},
								"sink": {
									"name": "CE_CASE_TYPE_ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['ce_case_type_version']"
								},
								"sink": {
									"name": "CE_CASE_TYPE_VERSION",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['ce_state_id']"
								},
								"sink": {
									"name": "CE_STATE_ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['ce_state_id']"
								},
								"sink": {
									"name": "CD_REFERENCE",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['Column_2']"
								}
							},
							{
								"source": {
									"path": "$['Column_3']"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "CcdMetadataSrc",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}