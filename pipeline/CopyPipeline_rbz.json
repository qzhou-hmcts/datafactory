{
	"name": "CopyPipeline_rbz",
	"properties": {
		"activities": [
			{
				"name": "Copy_rbz",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "ccd-asylum//"
					},
					{
						"name": "Destination",
						"value": "dbo.ccd"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": "*",
							"wildcardFileName": "*.json"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['extraction_date']"
								},
								"sink": {
									"name": "extraction_date",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['case_metadata_event_id']"
								},
								"sink": {
									"name": "case_metadata_event_id",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "$['ce_case_data_id']"
								},
								"sink": {
									"name": "ce_case_data_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['ce_created_date']"
								},
								"sink": {
									"name": "ce_created_date",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "$['ce_case_type_id']"
								},
								"sink": {
									"name": "ce_case_type_id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['ce_case_type_version']"
								},
								"sink": {
									"name": "ce_case_type_version",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['ce_state_id']"
								},
								"sink": {
									"name": "ce_state_id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['ce_data']['appealType']"
								},
								"sink": {
									"name": "cd_appealType",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['ce_data']['hasNewMatters']"
								},
								"sink": {
									"name": "cd_hasNewMatters",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "CcdMetadataSrc",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "synapse_stg_ccd_metadata",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}