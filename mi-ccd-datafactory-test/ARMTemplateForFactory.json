{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mi-ccd-datafactory-test"
		},
		"MI_synapse_mi_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MI_synapse_mi'"
		},
		"mi_synapse_kv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mi_synapse_kv'"
		},
		"mi_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mi-vault-test.vault.azure.net/"
		},
		"mi_persistence_storage_mi_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://mipersistenttest.blob.core.windows.net"
		},
		"mi_persistent_storage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "mi-staging-storage-connection-string"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/load_payment_bs_envelope_case_pipeline')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "copy_payment_bs_00",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "JsonSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"modifiedDatetimeStart": {
										"value": "@startOfDay(getPastTime(1,'Week'))",
										"type": "Expression"
									},
									"modifiedDatetimeEnd": {
										"value": "@utcnow()",
										"type": "Expression"
									},
									"wildcardFileName": "*.jsonl"
								},
								"formatSettings": {
									"type": "JsonReadSettings"
								}
							},
							"sink": {
								"type": "SqlDWSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['id']"
										},
										"sink": {
											"name": "id",
											"type": "Decimal"
										}
									},
									{
										"source": {
											"path": "$['ccd_reference']"
										},
										"sink": {
											"name": "ccd_reference",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['date_created']"
										},
										"sink": {
											"name": "date_created",
											"type": "DateTime"
										}
									},
									{
										"source": {
											"path": "$['exception_record_reference']"
										},
										"sink": {
											"name": "exception_record_reference",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['date_updated']"
										},
										"sink": {
											"name": "data_updated",
											"type": "DateTime"
										}
									},
									{
										"source": {
											"path": "$['envelope_id']"
										},
										"sink": {
											"name": "envelpe_id",
											"type": "Decimal"
										}
									},
									{
										"source": {
											"path": "$['extraction_date']"
										},
										"sink": {
											"name": "extraction_date",
											"type": "DateTime"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "payment_bs_envelope_case_src",
								"type": "DatasetReference",
								"parameters": {
									"file": "*"
								}
							}
						],
						"outputs": [
							{
								"referenceName": "synapse_payment_bs_envelope_case",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/payment_bs_envelope_case_src')]",
				"[concat(variables('factoryId'), '/datasets/synapse_payment_bs_envelope_case')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/payment_bs_envelope_case_src')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Payment bulk envelope case container",
				"linkedServiceName": {
					"referenceName": "mi_persistent_storage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"file": {
						"type": "string",
						"defaultValue": "*"
					}
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "@dataset().file",
							"type": "Expression"
						},
						"container": "payment-bs-envelope-case"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"id": {
							"type": "integer"
						},
						"ccd_reference": {
							"type": "string"
						},
						"date_created": {
							"type": "string"
						},
						"exception_record_reference": {
							"type": "string"
						},
						"date_updated": {
							"type": "string"
						},
						"envelope_id": {
							"type": "integer"
						},
						"extraction_date": {
							"type": "string"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/mi_persistent_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/synapse_payment_bs_envelope_case')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Payment  bs envelope case table on synapse",
				"linkedServiceName": {
					"referenceName": "MI_synapse_mi",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlDWTable",
				"schema": [],
				"typeProperties": {
					"schema": "payment",
					"table": "BulkEnvelopCase"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/MI_synapse_mi')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MI_synapse_mi')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"connectionString": "[parameters('MI_synapse_mi_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/mi_keyvault')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('mi_keyvault_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/mi_persistence_storage_mi')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"serviceEndpoint": "[parameters('mi_persistence_storage_mi_properties_typeProperties_serviceEndpoint')]",
					"accountKind": "StorageV2"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/mi_persistent_storage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "mi_keyvault",
							"type": "LinkedServiceReference"
						},
						"secretName": "[parameters('mi_persistent_storage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/mi_keyvault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/mi_synapse_kv')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"connectionString": "[parameters('mi_synapse_kv_connectionString')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "mi_keyvault",
							"type": "LinkedServiceReference"
						},
						"secretName": "mi-datawarehouse-server-admin-password"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/mi_keyvault')]"
			]
		}
	]
}