{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mi-ccd-datafactory-test"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CcdMetadataSrc')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "mi_persistent_storage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"file": {
						"type": "string",
						"defaultValue": "*"
					}
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "@dataset().file",
							"type": "Expression"
						},
						"container": "payment-bs-envelope"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"extraction_date": {
							"type": "string"
						},
						"case_metadata_event_id": {
							"type": "integer"
						},
						"ce_case_data_id": {
							"type": "integer"
						},
						"ce_created_date": {
							"type": "integer"
						},
						"ce_case_type_id": {
							"type": "string"
						},
						"ce_case_type_version": {
							"type": "integer"
						},
						"ce_state_id": {
							"type": "string"
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/synapse_stg_ccd_metadata')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "MI_synapse_mi",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlDWTable",
				"schema": [],
				"typeProperties": {
					"schema": "data",
					"table": "change"
				}
			},
			"dependsOn": []
		}
	]
}